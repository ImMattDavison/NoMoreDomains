{
  "manifest_version": 2,
  "name": "No More Domains",
  "version": "1.0.0",
  "description": "Need to stop buying domain names? No More Domains blocks all the best known domain registrars and domain name auction sites to help you stop buying so many domains!",
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["blockpage.js"]
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "scripting",
    "storage",
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self'; script-src-elem 'self'; object-src 'self'"
  
}
